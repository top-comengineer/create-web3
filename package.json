{
  "name": "create-web3",
  "version": "0.0.0",
  "private": true,
  "keywords": [
    "create-web3",
    "web3",
    "boilerplate",
    "nextjs",
    "react",
    "wagmi",
    "hardhat"
  ],
  "scripts": {
    "dev": "yarn workspace @create-web3/next dev",
    "build": "yarn workspace @create-web3/next build",
    "start": "yarn workspace @create-web3/next start",
    "lint": "yarn workspace @create-web3/next lint",
    "dev:ts": "yarn workspace @create-web3/next-ts dev",
    "build:ts": "yarn workspace @create-web3/next-ts build",
    "start:ts": "yarn workspace @create-web3/next-ts start",
    "lint:ts": "yarn workspace @create-web3/next-ts lint",
    "dev:vite": "yarn workspace @create-web3/vite dev",
    "build:vite": "yarn workspace @create-web3/vite build",
    "preview:vite": "yarn workspace @create-web3/vite preview",
    "dev:vite:ts": "yarn workspace @create-web3/vite-ts dev",
    "build:vite:ts": "yarn workspace @create-web3/vite-ts build",
    "preview:vite:ts": "yarn workspace @create-web3/vite-ts preview",
    "chain": "yarn workspace @create-web3/hardhat chain",
    "clean": "yarn workspace @create-web3/hardhat clean",
    "deploy": "yarn workspace @create-web3/hardhat deploy",
    "deploy:rinkeby": "yarn workspace @create-web3/hardhat deploy:rinkeby",
    "deploy:mainnet": "yarn workspace @create-web3/hardhat deploy:mainnet",
    "chain:ts": "yarn workspace @create-web3/hardhat-ts chain",
    "clean:ts": "yarn workspace @create-web3/hardhat-ts clean",
    "deploy:ts": "yarn workspace @create-web3/hardhat-ts deploy",
    "chain:foundry": "yarn workspace @create-web3/foundry chain",
    "compile:foundry": "yarn workspace @create-web3/foundry compile",
    "test:foundry": "yarn workspace @create-web3/foundry test",
    "clean:foundry": "yarn workspace @create-web3/foundry clean",
    "update-version": "lerna version --force-publish --no-private",
    "publish-latest": "lerna publish from-package",
    "create-web3": "node ./packages/create-web3/index.js",
    "dev:docs": "yarn workspace @create-web3-docs/docs dev"
  },
  "devDependencies": {
    "lerna": "^4.0.0"
  },
  "workspaces": [
    "packages/*",
    "packages/create-web3/*",
    "packages/create-web3/templates/foundry/*",
    "packages/create-web3/templates/hardhat/*",
    "packages/create-web3/templates/next/*",
    "packages/create-web3/templates/vite/*",
    "docs/"
  ]
}
